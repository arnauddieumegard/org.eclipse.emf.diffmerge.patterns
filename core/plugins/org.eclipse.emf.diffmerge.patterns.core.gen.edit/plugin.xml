<plugin>
    
  <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
    <factory 
       uri = "http://org.eclipse.com/emf/diffmerge/patterns/core/1.0.0" 
       class = "org.eclipse.emf.diffmerge.patterns.core.gen.corepatterns.provider.CorepatternsItemProviderAdapterFactory" 
       supportedTypes = 
         "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
