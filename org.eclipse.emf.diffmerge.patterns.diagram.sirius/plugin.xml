         point="org.eclipse.emf.diffmerge.patterns.core.genericTypeUtil">
      <client
            class="org.eclipse.emf.diffmerge.patterns.diagram.sirius.util.SiriusGenericTypeUtil">
      </client>
   </extension>
   
<!-- Diagram Util implementation Extension -->
   
   <extension
         point="org.eclipse.emf.diffmerge.patterns.diagram.diagramUtil">
      <client
            class="org.eclipse.emf.diffmerge.patterns.diagram.sirius.util.SiriusDiagramUtil"></client>
