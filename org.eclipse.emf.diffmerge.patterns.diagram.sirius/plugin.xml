<!-- Operation Factory Extension -->

 <extension
                point="org.eclipse.emf.diffmerge.patterns.diagram.operationFactory">
             <client
                   class="org.eclipse.emf.diffmerge.patterns.diagram.sirius.factories.SiriusPatternOperationFactory">
             </client>
 </extension>

</plugin>
